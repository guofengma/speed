body {
  padding-bottom: 120rpx;
  background-color: #F2F2F2;
}
.user_info_box {
  background-color: #1e82d2;
}
.user_info_box .user_info_avatar {
  width: 200rpx;
  height: 200rpx;
}
.user_info_box .user_info_avatar > image {
  display: block;
  margin: 20rpx auto;
  width: 160rpx;
  height: 160rpx;
  border-radius: 50%;
  overflow: hidden;
}
.user_info_box .user_info {
  padding: 20rpx 0;
  width: 310rpx;
}
.user_info_box .user_info .user_info_identifying,
.user_info_box .user_info .user_info_name {
  line-height: 80rpx;
  font-size:32rpx;
  color: #fff;
}
.user_info_box .user_info_view {
  width: 240rpx;
  height: 200rpx;
  position: relative;
}
.user_info_box .user_info_view .user_info_jurisdiction {
  text-indent: 100rpx;
  font-size: 32rpx;
  line-height: 60rpx;
  position: absolute;
  top: 30rpx;
  left: 0px;
  width: 100%;
  height: 60rpx;
  background-color: #fff;
  border-radius: 60rpx 0 0 60rpx;
}
.user_info_box .user_info_view .user_info_jurisdiction:after {
  content: '';
  position: absolute;
  top: 50%;
  left: 30rpx;
  transform: translateY(-50%);
  background-size: 100% 100%;
  width: 48rpx;
  height: 48rpx;
  background-image: url('data:image/png;base64,');
}
.user_info_box .user_info_view .user_info_go_icon {
  position: absolute;
  left: 140rpx;
  top: 120rpx;
  transform: rotateZ(180deg);
  width: 20rpx;
  height: 36rpx;
  background-size: 100% 100%;
  background-image: url('data:image/png;base64,');
}
.my_list_box .my_list view {
  background-color: #fff;
}
.my_list_box .my_list view:nth-of-type(n+2) {
  margin-top: 10rpx;
}
.my_list_box .my_list view:nth-of-type(1) .my_item_major .my_item_major_icon::after {
  background-image: url('data:image/png;base64,');
}
.my_list_box .my_list view:nth-of-type(1) .my_item_num {
  color: #16A8E1;
}
.my_list_box .my_list view:nth-of-type(2) .my_item_major .my_item_major_icon::after {
  background-image: url('data:image/png;base64,');
}
.my_list_box .my_list view:nth-of-type(2) .my_item_num {
  color: #FF6652;
}
.my_list_box .my_list view:nth-of-type(3) .my_item_major .my_item_major_icon::after {
  width:  40rpx;
  height: 40rpx;
  background-image: url('data:image/png;base64,');
}
.my_list_box .my_list view:nth-of-type(3) .my_item_num {
  color: #FF0052;
}
.my_list_box .my_list > view > view {
  width: 25%;
  font-size: 32rpx;
  text-align: center;
}
.my_list_box .my_list view .my_item_major {
  box-sizing: border-box;
  border-right: 1px solid #cccccc;
  position: relative;
}
.my_list_box .my_list view .my_item_major .my_item_major_icon {
  line-height: 60rpx;
  height: 60rpx;
}
.my_list_box .my_list view .my_item_major .my_item_major_icon::after {
  content: '';
  position: absolute;
  top: 10rpx;
  left: 50%;
  width: 48rpx;
  height: 48rpx;
  transform: translateX(-50%);
  background-size: 100% 100%;
}
.my_list_box .my_list view .my_item_major .my_item_text {
  line-height: 40rpx;
}
.my_list_box .my_list view .my_item .my_item_num {
  line-height: 60rpx;
  height: 60rpx;
}
.my_list_box .my_list view .my_item .my_item_text {
  line-height: 40rpx;
}
.fn_list_box .fn_list {
  font-size: 32rpx;
}
.fn_list_box .fn_list .fn_item {
  background-color: #fff;
  margin-top: 10rpx;
  line-height: 80rpx;
  position: relative;
}
.fn_list_box .fn_list .fn_item::before {
  content: '';
  position: absolute;
  top: 50%;
  right: 20rpx;
  width: 48rpx;
  height: 48rpx;
  transform: translateY(-50%);
  background-size: 100% 100%;
  background-image: url('data:image/png;base64,');
}
.fn_list_box .fn_list .fn_item span {
  float: left;
  text-indent: 1em;
}
.fn_list_box .fn_list .fn_item span:nth-of-type(2) {
  margin-right: 80rpx;
  float: right;
}
.fn_list_box .academy {
  background-color: #fff;
  font-size: 32rpx;
}
.fn_list_box .academy .academy_title {
  margin-top: 10rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #9c9999;
}
.fn_list_box .academy .academy_title view {
  text-indent: 1em;
  line-height: 80rpx;
}
.fn_list_box .academy .academy_title .more {
  width: 140rpx;
  color: #9c9999;
}
.fn_list_box .academy .academy_info .academy_info_img {
  padding: 15rpx 40rpx;
  width: 220rpx;
  height: 220rpx;
}
.fn_list_box .academy .academy_info .academy_info_img > image {
  display: block;
  width: 100%;
  height: 100%;
}
.fn_list_box .academy .academy_info .academy_info_detail {
  padding: 15rpx 0;
  width: 450rpx;
}
.fn_list_box .academy .academy_info .academy_info_detail .academy_info_detail_title {
  font-weight: 700;
}
.fn_list_box .academy .academy_info .academy_info_detail .academy_info_detail_content_box .academy_info_detail_content_avatar {
  padding: 5rpx;
  width: 50rpx;
  height: 50rpx;
}
.fn_list_box .academy .academy_info .academy_info_detail .academy_info_detail_content_box .academy_info_detail_content_avatar > image {
  display: block;
  width: 100%;
  height: 100%;
  border-radius: 50%;
  overflow: hidden;
}
.fn_list_box .academy .academy_info .academy_info_detail .academy_info_detail_content_box .academy_info_detail_content_name {
  font-weight: 700;
  text-indent: 20rpx;
  width: 190rpx;
  line-height: 60rpx;
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  color: #545454;
}
.fn_list_box .academy .academy_info .academy_info_detail .academy_info_detail_content_box .academy_info_detail_content_time {
  width: 100px;
  line-height: 60rpx;
  text-align: center;
  color: #545454;
}
.fn_list_box .academy .academy_info .academy_info_detail .academy_info_detail_content_box .academy_info_detail_content {
  line-height: 40rpx;
  height: 120rpx;
  overflow: hidden;
  color: #545454;
}
